<h1>Actualizar Torre</h1>

<%= form_for @tower do |f| %>
<%= link_to "ver todas", tower_path %>

<p>
    <%= f.label :name, "Nombre" %><br>
    <%= f.text_area :name %>
</p>

 <p>
    <%= f.label :keeper, "Portero" %><br>
    <%= f.text_area :keeper %>
 </p>

 <p>
    <%= f.submit %>
 </p>
<% end %>